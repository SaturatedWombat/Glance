{
  "manifest_version": 2,

  "name": "Glance",
  "description": "The simple social network",
  "version": "1.0",

  "chrome_url_overrides" : {
      "newtab": "newtab.html"
    },

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
